#include<iostream>
#include<vector>
using namespace std;

int main(){
    vector<int>arr = {1,2,3,3,3,4,5,6,7,8};
    int k = 3;

    int left = 0;
    int right = arr.size()-1;
    int mid = left + right / 2;

    while(left < right){
        if(arr[mid] == k){
            break;
        }
        else if(arr[mid] < k){
            mid = left;
        }
        else{
            mid = right;
        }
    }

    while(arr[mid] == arr[mid-1]){
        mid--;
    }
    cout<<mid;

}